{
  "name": "@sap/cds-runtime",
  "version": "1.2.8",
  "description": "",
  "main": "lib/cds-services",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test-nong": "CDS_FEATURES_NONG=y jest --config=jest-nong.json --testTimeout=10000",
    "jsdoc2md": "jsdoc2md --param-list-format list \"lib/**/*.js\" > docs/api.md"
  },
  "files": [
    "lib",
    "changelogs"
  ],
  "engines": {
    "node": ">= 8.9.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run jsdoc2md && git add docs/api.md"
    }
  },
  "lint-staged": {
    "{lib,test}/**/*.js": [
      "prettier-standard",
      "standard --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@sap/cds-foss": "^1.1.0",
    "@sap/odata-server": "^1.6.1",
    "@sap-cloud-sdk/core": "1.18.1"
  }
}